<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BasquetballCount.Views.HomaPage">
    <ContentPage.Content>
        <StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand" Spacing="20">
            <Button Text="Go to match" Command="{Binding GoToCountPageCommand}"/>
            <Button Text="Edit Teams" Command="{Binding GoToEditTeamsCommand}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>